{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "blog_series",
  "info": {
    "name": "Blog Series",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "blog_posts": {
      "collection": "blog-post",
      "via": "blog_series"
    },
    "blog_post_categories": {
      "collection": "blog-post-category",
      "via": "blog_series",
      "dominant": true
    },
    "image_header": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "public": {
      "type": "boolean"
    },
    "manually_published_at": {
      "type": "datetime"
    },
    "authors": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "blog_series",
      "dominant": true
    },
    "background_color": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    }
  }
}
